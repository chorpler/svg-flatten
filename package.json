{
  "name": "svg-flatten",
  "version": "2.0.0",
  "description": "Turns SVG shapes (polygon, polyline, rect, g) into SVG paths. It can merge groups and apply transforms.",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.es.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.browser.js",
  "types": "src/index.ts",
  "bin": {
    "svgflatten": "./build/cli.js"
  },
  "files": [
    "build",
    "compiled",
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf build/* lib/* dist/* compiled/* && mkdirp build lib dist compiled",
    "clrscr": "node -e \"process.stdout.write('\\x1B[2J\\x1B[0f')\"",
    "declarations": "tsc -p ./tsconfig.json --emitDeclarationOnly",
    "rollup-cjs": "rollup -c config/rollup.config.cjs.js && rollup -c config/rollup.config.browser.cjs.js",
    "rollup-es": "rollup -c config/rollup.config.es.js && rollup -c config/rollup.config.browser.es.js",
    "rollup": "npm-run-all --parallel rollup-cjs rollup-es",
    "browserify:rollup": "browserify -t [ babelify --global --presets [ @babel/preset-env ] ] -t brfs -p bundle-collapser/plugin -s SvgMod lib/index.browser.js > dist/svgmod.js",
    "browserify": "npm run browserify:rollup",
    "minify": "uglifyjs -mc < dist/svgmod.js > dist/svgmod.min.js",
    "build": "npm run build:prod",
    "build:dev": "npm-run-all clean rollup browserify minify",
    "build:prod": "npm-run-all clean rollup browserify minify",
    "prepublishOnly": "npm run build",
    "compile": "tsc -p ./tsconfig.json",
    "lint": "jshint **/*.js",
    "testOriginal": "mocha",
    "testold": "ts-mocha",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "test-types": "tsc --noEmit -p ./tsconfig.json",
    "flatten": "node build/lib.js"
  },
  "dependencies": {
    "boxen": "^5.0.1",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "graceful-fs": "^4.2.8",
    "node-ts": "^5.1.2",
    "svgpath": "^2.1.6",
    "ts-node": "^10.2.1",
    "xmldoc": "^0.5.0",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/chai": "^4.2.21",
    "@types/xmldoc": "^1.1.6",
    "@types/yargs": "^17.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-register": "^6.26.0",
    "babelify": "^10.0.0",
    "brfs": "^2.0.2",
    "browserify": "^17.0.0",
    "bundle-collapser": "^1.4.0",
    "chai": "^4.3.4",
    "core-js": "^3.17.3",
    "cross-env": "^7.0.3",
    "fast-xml-parser": "^3.20.0",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-inject": "^3.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^11.1.2",
    "standard-version": "^9.3.1",
    "ts-mocha": "^10.0.0",
    "ts-mock-imports": "^1.3.7",
    "typescript": "^4.4.3",
    "uglify-js": "^3.14.2",
    "watchify": "^4.0.0"
  },
  "packageComments": {
    "main": "lib/index.js"
  },
  "keywords": [
    "svg",
    "flatten",
    "flattener",
    "shape",
    "circle",
    "ellipse",
    "line",
    "path",
    "polygon",
    "polyline",
    "rect",
    "g",
    "transform"
  ],
  "author": "David Sargeant",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chorpler/svg-flatten.git"
  },
  "standard-versionOriginal": {
    "scripts": {
      "postbump": "git checkout -- bower.json"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "homepage": "https://github.com/chorpler/svg-flatten#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "devDependenciesOriginal": {
    "chai": "^3.5.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.9",
    "sinon": "^1.17.4"
  },
  "scriptsOriginal": {
    "test": "mocha",
    "lint": "jshint **/*.js"
  },
  "oldHelpers": {
    "main": "lib/index.js",
    "jsnext:main": "lib/index.es.js",
    "module": "lib/index.es.js",
    "browser": "lib/index.browser.js",
    "types": "src/index.ts",
    "bin": {
      "svgflatten": "./build/cli.js"
    }
  }
}
